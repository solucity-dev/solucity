{
  "name": "@solucity/backend",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/server.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts --max-warnings=999",
    "format": "prettier . --write",
    "generate": "prisma generate",
    "postinstall": "prisma generate",
    "db:clean": "tsx prisma/cleanDb.ts",
    "db:seed": "prisma db seed",
    "studio": "prisma studio",
    "jobs:ssi": "tsx src/jobs/backfill-ssi.ts",
    "seed:base": "tsx src/seeds/ensureBaseServices.ts",
    "seed:once": "tsx src/seeds/run-once.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^17.3.1",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.1.0",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1",
    "resend": "^6.9.3",
    "sharp": "^0.34.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.19",
    "@types/node-fetch": "^2.6.13",
    "@types/nodemailer": "^7.0.3",
    "prisma": "^6.17.0",
    "tsx": "^4.20.6",
    "typescript": "~5.9.2"
  }
}
